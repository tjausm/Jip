class Main {

  static int isOdd(int n) {
    if (n == 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    } else {
        int isEven := Main.isEven(n - 1);
        return isEven;
    }
  }

  static int isEven(int n) {
    if (n == 0) {
        return 1;
    } else if (n == 1) {
        return 0;
    } else {
        int isOdd := Main.isOdd(n - 1);
        return isOdd;
    }
  }

  static void main(int n) {
    if (n < 0) {
      return;
    };
    int result := Main.isEven(n);
    int mod := n % 2;
    if (result < 0 || result == mod) {
      return;
    } else {
      assert false;
    }
  }
}
