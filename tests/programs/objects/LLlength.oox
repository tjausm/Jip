//
// Linked-list, with several methods.
//
class Node {
    -int value ;
    -Node next ;

    Node(int value, Node next){
        this.value := value;
        this.next := next;
    }

    bool member(int x) {
        int v := this.value ;
        
        if(x==v) return true ;
        else {
            Node n := this.next ;
            bool b := n.member(x) ;
            return b ;
        };
    }

    int length() {
        Node n := this.next ;
        if (n==null) return 1 ;
        else {
            int k := n.length() ;
            return k+1 ;
        };
    }

}
