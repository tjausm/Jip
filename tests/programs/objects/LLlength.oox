class Main {
    
    static void main(Node p){
        int len := p.length();
        assert len == 1;


    }
}

//
// Linked-list, with several methods.
//
class Node {
    -int value ;
    -Node next ;

    Node(int value, Node next){
        this.value := value;
        this.next := next;
    }

    int length() {
        Node n := this.next;
        if (n==null) return 1 ;
        else {
            int k := n.length() ;
            return k+1 ;
        };
    }

}
