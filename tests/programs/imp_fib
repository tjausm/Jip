class Main
{
    static void main(int n)
    {
        assume n >= 0;
        int fb := Main.impFib(n) ;
    }

    static int impFib(int n) {
       if (n == 0) {return 0;}
       else {if (n == 1) return 1;
       else {
          int total := 1 ;
          int mature := 0 ;
          int t := 1 ;
          while (t<n) {
             int newborn := mature ;
             mature := total ;
             total := total + newborn ;
          }
          return total ;
       }}
    }

}
