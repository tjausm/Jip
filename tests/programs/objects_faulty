class Main {
    static void main(int x){
        Counter c := new Counter(x - 30);
        c.increment();
        c.increment();
        c.increment();
        c.decrement();
        int cvalue := c.count;
        assert cvalue == 10;
    }
}

class Counter {
    -int count;
    Counter(int initial) {this.count := initial; }
    void increment() {int i := this.count; this.count := i + 1;}
    void decrement() {int i := this.count; this.count := i - 1;}
}