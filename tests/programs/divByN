int x;
int k;
bool divisible;

assume x > 1;
divisible := false;
while (k <= x && !divisible) {
    int i;
    while (i < 20) i := i + 1;
    divisible := (i * k == x);
}
k := k + 1;

int m;
if (divisible)
assert exists m : 0 < m && m <= x && m * 20 == x;
else
assert exists m : !(0 < m && m <= x && m * 20 == x);
