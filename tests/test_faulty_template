#[allow(non_snake_case)]
#[test]
fn {name}() {{
    let mut cmd = Command::cargo_bin("jip").unwrap();
    let assert = cmd
        .arg("file")
        .arg(r#"{path}"#)
        .arg("verify")
        .assert();
    assert
        .code(1);
        
    // repeat test with simplifier
    let mut cmd_simple = Command::cargo_bin("jip").unwrap();
    let assert_simple = cmd_simple
        .arg("file")
        .arg(r#"{path}"#)
        .arg("verify")
        .arg("-s")
        .assert();
    assert_simple
        .code(1);
}}